{
  "name": "ydx-ai-backend",
  "version": "1.0.0",
  "description": "YouDescribeX Backend Server",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "prepare": "husky install",
    "format": "prettier --write \"./src/**/*.{ts,js}\" --config .prettierrc"
  },
  "lint-staged": {
    "*.ts": [
      "npx prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhavani-goruganthu/ydx-ai-backend.git"
  },
  "author": "YouDescribe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bhavani-goruganthu/ydx-ai-backend/issues"
  },
  "homepage": "https://github.com/bhavani-goruganthu/ydx-ai-backend#readme",
  "dependencies": {
    "@google-cloud/text-to-speech": "^3.4.0",
    "@types/string-similarity": "^4.0.2",
    "axios": "^1.5.0",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "express": "^4.17.3",
    "get-mp3-duration": "^1.0.0",
    "js-yaml": "^4.1.0",
    "memory-cache": "^0.2.0",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "mongoose": "^6.9.1",
    "multer": "^1.4.4-lts.1",
    "nodemailer": "^6.9.4",
    "passport": "^0.5.3",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^8.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.16.2",
    "sequelize-typescript": "^2.1.5",
    "string-similarity": "^4.0.4",
    "swagger-jsdoc": "5.0.1",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.220",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.13.0",
    "@types/nodemailer": "^6.4.9",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.35",
    "@types/sequelize": "^4.28.14",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.52.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.9.5"
  }
}
